# Environment Variables for Bio-Block Next.js Frontend
# Copy this file to .env.local and fill in your actual values

# Backend API URL (JavaScript backend)
NEXT_PUBLIC_JS_BACKEND_URL=http://localhost:3001

# 32-byte encryption key for AES-256 encryption
# Generate using: openssl rand -hex 32
NEXT_PUBLIC_ENCRYPTION_KEY=your_32_byte_encryption_key_here
